{
	"nodes": [
		{
			"id": "assistant",
			"type": "streamingaiassistantnode",
			"docks": {
				"chatmodel": [ "router" ],
				"memory": [ "sessionmemory", "timememory" ],
				"tools": [ "webfetch", "helloworld", "canvasclose" ],
				"logger": [ "log" ]
			}
		}
	],
	"resources": [
		{
			"id": "router",
			"type": "routingchatmodelagentresource",
			"config": {
				"strategy": { "mode": "fixed", "value": "failover" },
				"sticky": { "mode": "fixed", "value": 1 },
				"stickymode": { "mode": "fixed", "value": "per_op" },
				"maxfailures": { "mode": "fixed", "value": 3 },
				"cooldownsec": { "mode": "fixed", "value": 120 },
				"targets": {
					"openai": { "tools": 1, "stream": 1, "weight": 1 },
					"qualitusai": { "tools": 0, "stream": 1, "weight": 1 }
				}
			},
			"docks": {
				"targets": ["openai", "qualitusai"],
				"logger": ["log"]
			}
		},
		{
                        "id": "openai",
                        "type": "openaichatmodelagentresource",
                        "config": {
                                "apikey": { "mode": "config", "section": "openai", "key": "apikey" },
                                "model": { "mode": "fixed", "value": "gpt-4o-mini" },
                                "temperature": { "mode": "fixed", "value": 0.3 }
                        }
                },
                {
                        "id": "qualitusai",
                        "type": "qualitusaichatproxyagentresource",
                        "config": {
                                "apikey": { "mode": "config", "section": "qualituschat", "key": "apikey" },
                                "model": { "mode": "fixed", "value": "Qwen/Qwen2.5-14B-Instruct-AWQ" },
                                "temperature": { "mode": "fixed", "value": 0.3 }
                        }
                },
		{
			"id": "sessionmemory",
			"type": "sessionmemoryagentresource",
			"config": {
				"max": { "mode": "fixed", "value": 5 }
			},
			"docks": {
				"logger": [ "log" ]
			}
		},
		{
			"id": "timememory",
			"type": "timememoryagentresource"
		},
		{
			"id": "webfetch",
			"type": "webfetchtextagenttool"
		},
		{
			"id": "helloworld",
			"type": "helloworldcanvasagenttool"
		},
		{
			"id": "canvasclose",
			"type": "canvascloseagenttool"
		},
		{
			"id": "log",
			"type": "loggerresource",
			"config": {
				"scope": { "mode": "fixed", "value": "reporting-chatbot" }
			}
		}
	],
	"connections": [
		{
			"from": "__input__",
			"output": "system",
			"to": "assistant",
			"input": "system"
		},
		{
			"from": "__input__",
			"output": "user",
			"to": "assistant",
			"input": "prompt"
		}
	]
}
